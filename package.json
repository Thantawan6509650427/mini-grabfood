{
  "name": "mini-grabfood",
  "version": "1.0.0",
  "description": "Restaurant rating app with Google OAuth authentication",
  "private": true,
  "scripts": {
    "install:all": "npm install --prefix backend && npm install --prefix frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --prefix backend",
    "dev:frontend": "npm run dev --prefix frontend",
    "start:backend": "npm start --prefix backend",
    "start:frontend": "npm start --prefix frontend",
    "db:setup": "mysql -u root -p < backend/database/schema.sql",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "npm test --prefix backend",
    "test:frontend": "npm test --prefix frontend",
    "test:watch": "concurrently \"npm run test:watch:backend\" \"npm run test:watch:frontend\"",
    "test:watch:backend": "npm run test:watch --prefix backend",
    "test:watch:frontend": "npm run test:watch --prefix frontend",
    "test:coverage": "npm run test:coverage:backend && npm run test:coverage:frontend",
    "test:coverage:backend": "npm run test:coverage --prefix backend",
    "test:coverage:frontend": "npm run test:coverage --prefix frontend"
  },
  "keywords": ["restaurant", "rating", "food", "fullstack", "react", "nodejs", "oauth"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}